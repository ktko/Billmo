package com.example.katieko.finalproject;

import android.os.Parcelable;

import java.util.ArrayList;

/**
 * Created by katieko on 4/18/17.
 */

public class PaymentPlan {
    private String title;
    private double totalAmount;
    private double tip;
    private String description;
    private ArrayList<String> payers;
    private String searchCode;

    public PaymentPlan() {
        payers = new ArrayList<>();
    }

    public PaymentPlan(String title, double totalAmount, double tip, String description, String searchCode) {
        this.title = title;
        this.totalAmount = totalAmount;
        this.tip = tip;
        this.description = description;
        this.searchCode = searchCode;
        payers = new ArrayList<>();
    }

    public String getTitle() {
        return title;
    }

    public double getTotalAmount() {
        return totalAmount;
    }

    public double getTip() {
        return tip;
    }

    public String getDescription() {
        return description;
    }

    public ArrayList<String> getPayers() {
        return payers;
    }

    public String getSearchCode() {
        return searchCode;
    }
}